{
	"publisher": "theNestruo",
	"name": "dark-minus-theme",
	"version": "2.0.0",
	"displayName": "Dark-",
	"description": "Simplified versions of the default Dark (Visual Studio) theme, with less syntax highlight",
	"categories": [
		"Themes"
	],
	"icon": "doc/images/icon.128.png",
	"galleryBanner": {
		"color": "1e1e1e",
		"theme": "dark"
	},
	"engines": {
		"vscode": "^1.81.0"
	},
	"contributes": {
		"themes": [
			{
				"label": "Dark- Modern",
				"uiTheme": "vs-dark",
				"path": "./themes/Dark- Modern.color-theme.json"
			},
			{
				"label": "Dark- Modern (Flat UI)",
				"uiTheme": "vs-dark",
				"path": "./themes/Dark- Modern (Flat UI).color-theme.json"
			},
			{
				"label": "Dark- (Visual Studio)",
				"uiTheme": "vs-dark",
				"path": "./themes/Dark-.color-theme.json"
			},
			{
				"label": "Dark- (Visual Studio) (Flat UI)",
				"uiTheme": "vs-dark",
				"path": "./themes/Dark- (Flat UI).color-theme.json"
			},
			{
				"label": "Dark-- (Visual Studio)",
				"uiTheme": "vs-dark",
				"path": "./themes/Dark--.color-theme.json"
			}
		]
	},
	"homepage": "https://github.com/theNestruo/dark-minus-theme-vscode",
	"bugs": {
		"url": "https://github.com/theNestruo/dark-minus-theme-vscode/issues"
	},
	"repository": {
		"type": "git",
